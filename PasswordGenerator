import random
uppercase="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
lowercase= uppercase.lower()
digits="0123456789"
symbols="@!#$%&*"
upper, lower, dig, sym=True, True, True, True
all=" "
if upper:
    all+=uppercase
if lower:
    all+=lowercase
if dig:
    all+=digits
if sym:
    all+=symbols
length=10
password="".join(random.sample(all,length))
print(password)
